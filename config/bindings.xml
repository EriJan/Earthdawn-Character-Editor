<?xml version="1.0" encoding="ISO-8859-1"?>
<jaxb:bindings version="2.0"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <jaxb:bindings schemaLocation="earthdawncharacter.xsd">
        <jaxb:globalBindings typesafeEnumMemberName="generateError" />
        <jaxb:bindings node="//xs:simpleType[@name='plusminus_type']">
            <jaxb:typesafeEnumClass>
                <jaxb:typesafeEnumMember value="+" name="PLUS" />
                <jaxb:typesafeEnumMember value="-" name="MINUS" />
            </jaxb:typesafeEnumClass>
        </jaxb:bindings>
        <jaxb:bindings node="//xs:simpleType[@name='janein_type']">
            <jaxb:typesafeEnumClass>
                <jaxb:typesafeEnumMember value="-" name="MINUS" />
                <jaxb:typesafeEnumMember value="*" name="STERN" />
            </jaxb:typesafeEnumClass>
        </jaxb:bindings>
        <jaxb:bindings node="//xs:simpleType[@name='geschlecht_type']">
            <jaxb:typesafeEnumClass>
                <jaxb:typesafeEnumMember value="-" name="MINUS" />
            </jaxb:typesafeEnumClass>
        </jaxb:bindings>
        <jaxb:bindings node="//xs:simpleType[@name='ATTRIBUT_name_type']">
            <jaxb:typesafeEnumClass>
                <jaxb:typesafeEnumMember value="Stärke" name="STAERKE" />
                <jaxb:typesafeEnumMember value="Zähigkeit" name="ZAEHIGKEIT" />
            </jaxb:typesafeEnumClass>
        </jaxb:bindings>
        <!-- jaxb:bindings node="//xs:simpleType[@name='disziplinlist_type']">
            <jaxb:typesafeEnumClass>
                <jaxb:typesafeEnumMember value="Läuterer" name="LAEUTERER" />
                <jaxb:typesafeEnumMember value="Schütze" name="SCHUETZE" />
            </jaxb:typesafeEnumClass>
        </jaxb:bindings-->
    </jaxb:bindings>
</jaxb:bindings>
